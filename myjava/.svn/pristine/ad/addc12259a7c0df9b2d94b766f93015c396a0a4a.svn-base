class Generics<T,K>{
	private T nameT;
	private K age;
	public Generics(T nameT, K age) {
		this.nameT=nameT;
		this.age=age;
	}
	public T getNameT() {
		return nameT;
	}
	public void setNameT(T nameT) {
		this.nameT = nameT;
	}
	public K getAge() {
		return age;
	}
	public void setAge(K age) {
		this.age = age;
	}
}
public class HelloGenerics {
	public static void main(String[] args) {
		Generics<String, Integer> G=new Generics<String, Integer>("er",10);
		System.out.println(G.getAge());
		System.out.println(G.getNameT());
	}

}
