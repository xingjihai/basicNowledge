package 基础知识.数组;

import java.util.Arrays;

public class ArraysTest {
    public static void main(String[] args) {
        int[] int_arr={4,5,1,3,2,9,6,8,7};
        Arrays.sort(int_arr);
        for (int i : int_arr) {
            System.out.print(i+",");
        }
        
        
        System.out.println("\r\n====================");
        
        int index=Arrays.binarySearch(int_arr, 9);
        System.out.println(index);
    }
}
