package 异常;

public class ExceptionClass extends Exception{
    private static final long serialVersionUID = -2169284765622001335L;
    private String msg;
    
    public ExceptionClass(String message) {
        this.msg=message;
    }

    public String getMsg() {
        return msg;
    }

    public void setMsg(String message) {
        this.msg = message;
    }
    
    
}
